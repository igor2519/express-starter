{
  "name": "greenparc-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node -r dotenv/config -r module-alias/register build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "build": "rimraf ./build && tsc -p tsconfig.json",
    "lint:fix": "eslint --fix --ext .js,.ts, src",
    "postinstall": "husky install",
    "setup:husky": "rimraf .husky/pre-commit .husky/pre-push && husky add .husky/pre-commit \"npx lint-staged\" && husky add .husky/pre-push \"npx tsc --noEmit\"",
    "setup": "yarn setup:husky && cpy --rename=.env example.env ./",
    "migration:create": "sequelize --migrations-path=./src/database/migrations/migrations migration:generate --name",
    "migration:up": "sequelize --config=./src/database/migrations/config.js --migrations-path=./src/database/migrations/migrations db:migrate",
    "migration:down": "sequelize --config=./src/database/migrations/config.js --migrations-path=./src/database/migrations/migrations db:migrate:undo",
    "migration:down:all": "sequelize --config=./src/database/migrations/config.js --migrations-path=./src/database/migrations/migrations db:migrate:undo:all",
    "seed:create": "sequelize --seeders-path=./src/database/seeders seed:generate --name",
    "seed:up": "sequelize --config=./src/database/migrations/config.js --seeders-path=./src/database/seeders db:seed:all",
    "seed:down": "sequelize --config=./src/database/migrations/config.js --seeders-path=./src/database/seeders db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express-form-data": "^2.0.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.19",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "pg": "^8.8.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.28.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cls-hooked": "^4.3.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "tsconfig-paths": "^4.1.2"
  },
  "_moduleAliases": {
    "@constants": "build/src/constants",
    "@controllers": "build/src/controllers",
    "@csv-parsers": "build/src/csv-parsers",
    "@types": "build/src/types",
    "@middlewares": "build/src/middlewares",
    "@repositories": "build/src/repositories",
    "@errors": "build/src/errors",
    "@routes": "build/src/routes",
    "@services": "build/src/services",
    "@utils": "build/src/utils",
    "@validation": "build/src/validation",
    "@dto-creators": "build/src/dto-creators",
    "@database": "build/src/database"
  },
  "lint-staged": {
    "*.ts": "tslint -c tslint.json -p tsconfig.json --fix",
    "*.{ts,js,css,md}": "prettier --config .prettierrc --write"
  }
}
